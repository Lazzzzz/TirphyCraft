public class BlockLaputaFairyFlower extends GrassBase {
	public BlockLaputaFairyFlower() {
		super("laputa_fairy_flower");

	}
	
	@Override
	public Item getItemDropped(IBlockState state, Random rand, int fortune)
    {
        return BlockInit.LAPUTA_FAIRY_FLOWER.getItemDropped(BlockInit.LAPUTA_FAIRY_FLOWER.getDefaultState(), rand, fortune);
    }
}
