package com.laz.tirphycraft.entity.render.boss;

import com.laz.tirphycraft.entity.boss.EntityArtikon;
import com.laz.tirphycraft.entity.boss.EntityPrimaryAttack;
import com.laz.tirphycraft.entity.boss.EntityPrimaryHeal;
import com.laz.tirphycraft.entity.model.boss.ModelArtikon;
import com.laz.tirphycraft.entity.model.boss.ModelPrimary;
import com.laz.tirphycraft.util.Reference;

import net.minecraft.client.renderer.entity.RenderLiving;
import net.minecraft.client.renderer.entity.RenderManager;
import net.minecraft.util.ResourceLocation;

public class RenderPrimaryStrength extends RenderLiving<EntityPrimaryHeal>{
	public static final ResourceLocation TEXTURES = new ResourceLocation(Reference.MOD_ID + ":textures/entity/primary_heal.png");
	
	public RenderPrimaryStrength(RenderManager manager) {
		super(manager, new ModelPrimary(), 1F);
	}
	
	@Override
	protected ResourceLocation getEntityTexture(EntityPrimaryHeal entity) {
		return TEXTURES;
	}
	
}
